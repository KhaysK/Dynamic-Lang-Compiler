# tuples behave like structures
#!name Tuples demonstration

var t = {a=10, b=20};
t.a = 25;

print t.a + 10; 

print t; 

# we can access tuple elements by order (starting from 1)
print t.1; # will print t.a value
print t.2; # will print t.b value

