# simple if statement
# available relations: <, >, <=, >=, ==, !=
if a <= b
then
	print "a is less than b or equal";
else
	print "a is greater than b";
end
	
# combined if statement
# available operators: and, or
if a > b and a > c
then
	print "a is greater than b and c";
end

if a < 5
