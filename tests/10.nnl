#!name Recursive fibbonaci numbers

const fib = func(n) do
    if n <= 2
    then
        return 1 + 0;
    else
        return fib(n - 1) + fib(n - 2);
    end
end

var i = 1;

while i <= 10
loop
    print fib(i);
    i = i + 1;
end

